for (let i = 1; i <= 50; i++) {
  console.log(i);
}
function findLargestNumber(num1, num2, num3) {
  return Math.max(num1, num2, num3);
}

const num1 = 10;
const num2 = 25;
const num3 = 15;

const largestNumber = findLargestNumber(num1, num2, num3);
console.log('The largest number is:', largestNumber);
